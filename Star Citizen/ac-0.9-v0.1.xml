    <script>-- CONSTANTS THAT YOU CAN CHANGE
FLIGHT_MODE		= 1	-- M1
ALTERNATIVE_MODE	= 2	-- M2
TO_ALTERNATIVE_KEY	= 24	-- G24

originalState = nil

function TestAndEnableAltMode(event, arg)
	if (GetMKeyState("lhc") == FLIGHT_MODE and event == "G_PRESSED" and arg == TO_ALTERNATIVE_KEY) then
		OutputLogMessage("entering in ENERGY mode\n");
		originalState = ALTERNATIVE_MODE
		SetMKeyState(ALTERNATIVE_MODE, "lhc")
	end
end

function TestAndDisableAltMode(event, arg)
	if (GetMKeyState("lhc") == ALTERNATIVE_MODE and event == "G_RELEASED" and arg == TO_ALTERNATIVE_KEY) then
		OutputLogMessage("leaving ENERGY mode\n");
		originalState = FLIGHT_MODE
		SetMKeyState(FLIGHT_MODE, "lhc")
	end
end

function OnEvent(event,arg,family)
--        	family = family or ""
	OutputLogMessage("event = %s, arg = %s\n", event, arg);
	if (family == "lhc") then
		TestAndEnableAltMode(event, arg)
		TestAndDisableAltMode(event, arg)
	end
end</script>
